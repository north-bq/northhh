<Window x:Class="TelekomNevaSvyaz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TelekomNevaSvyaz"
        mc:Ignorable="d"
        MinWidth="800" MinHeight="450"
        WindowState="Maximized"
        Title="ООО &quot;Телеком Нева Связь&quot;" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="120"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Image Source="/Resources/Лого ТНС.jpg"  Margin="10"/>
            <TextBlock Text="ООО Телеком Нева Связь" FontSize="25" Margin="10"/>
        </StackPanel>
        <Frame Name="FrNav" Grid.Row="1"/>
    </Grid>
</Window>
